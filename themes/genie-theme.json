{
  "$schema": "../schemas/colour-theme.json",
  "name": "Genie Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#0F0F2A",
    "editor.foreground": "#B0B0FF",
    "editorCursor.foreground": "#00BFFF",
    "editor.lineHighlightBackground": "#1E1E3F",
    "editor.selectionBackground": "#003366",
    "editor.selectionHighlightBackground": "#003366",
    "editorIndentGuide.background1": "#1E1E3F",
    "editorIndentGuide.activeBackground1": "#00BFFF",
    "editorWhitespace.foreground": "#003366",
    "activityBar.background": "#0F0F2A",
    "sideBar.background": "#0F0F2A",
    "statusBar.background": "#1E1E3F",
    "titleBar.activeBackground": "#0F0F2A",
    "titleBar.inactiveBackground": "#0F0F2A",
    "titleBar.activeForeground": "#00BFFF",
    "titleBar.inactiveForeground": "#003366",
    "input.background": "#1E1E3F",
    "input.foreground": "#B0B0FF",
    "button.background": "#FFD700",
    "button.foreground": "#000000",
    "tab.activeBackground": "#0F0F2A",
    "tab.inactiveBackground": "#1E1E3F",
    "tab.activeBorder": "#FFD700",
    "tab.hoverBackground": "#003366",
    "tab.hoverBorder": "#00BFFF",
    "peekViewEditor.background": "#0F0F2A",
    "peekViewEditor.matchHighlightBackground": "#003366",
    "peekViewResult.background": "#0F0F2A",
    "peekViewResult.matchHighlightBackground": "#003366",
    "peekViewTitle.background": "#0F0F2A",
    "peekViewTitleLabel.foreground": "#B0B0FF",
    "editorHoverWidget.background": "#1E1E3F",
    "editorHoverWidget.border": "#00BFFF",
    "editorHoverWidget.foreground": "#B0B0FF",
    "editorHoverWidget.highlightForeground": "#FFD700",
    "editorGroupHeader.tabsBackground": "#0F0F2A",
    "quickInput.background": "#0F0F2A",
    "quickInput.foreground": "#B0B0FF",
    "quickInputList.focusBackground": "#FFD700",
    "quickInputList.focusForeground": "#0F0F2A"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "comment.block.documentation",
        "comment.block.documentation keyword",
        "comment.block.documentation string"
      ],
      "settings": {
        "foreground": "#8A2BE2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.type"
      ],
      "settings": {
        "foreground": "#FF007F"
      }
    },
    {
      "scope": [
        "constant",
        "constant.language",
        "constant.numeric",
        "constant.character",
        "constant.other",
        "entity.name.constant",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "variable.language"
      ],
      "settings": {
        "foreground": "#1E90FF"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#e486fc"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.name.class",
        "entity.name.namespace",
        "storage.type.class",
        "storage.type.struct",
        "entity.name.enum",
        "storage.type.annotation",
        "entity.other.inherited-class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF4500"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.italic",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#B0B0FF"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#00BFFF"
      }
    },
    {
      "scope": [
        "markup.strike"
      ],
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#4B0082"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#003366"
      }
    },
    {
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.title.markdown",
        "string.other.link.anchor.markdown"
      ],
      "settings": {
        "foreground": "#BA55D3"
      }
    },
    {
      "scope": [
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "scope": [
        "markup.fenced_code.block",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "scope": [
        "markup.fenced_code.block.markdown variable"
      ],
      "settings": {
        "foreground": "#FF4500"
      }
    }
  ]
}
